{
  "name": "VegaLabs/contract",
  "version": "0.1",
  "description": "VegaLabs",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "truffle compile -all",
    "test": "truffle test",
    "lint": "npm run lint:sol",
    "lint:fix": "npm run lint:sol:fix",
    "lint:sol": "solhint 'contracts/**/*.sol' && prettier -c 'contracts/**/*.sol'",
    "lint:sol:fix": "prettier --write \"contracts/**/*.sol\""
  },
  "keywords": [
    "Vega Labs",
    "Ethereum",
    "ERC-20"
  ],
  "author": "Jackie",
  "license": "MIT",
  "devDependencies": {
    "@ethersproject/units": "^5.7.0",
    "@openzeppelin/contracts": "^4.8.1",
    "@truffle/hdwallet-provider": "^2.0.13",
    "dotenv": "^16.0.1",
    "erc721a": "^4.2.3",
    "ganache-time-traveler": "^1.0.16",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "solhint": "^3.3.7",
    "truffle-assertions": "^0.9.2",
    "truffle-plugin-verify": "^0.5.28"
  },
  "dependencies": {
    "@ethereumjs/common": "^3.0.1",
    "@openzeppelin/contracts-upgradeable": "^4.8.0",
    "abdk-libraries-solidity": "^3.0.0",
    "ethereumjs-tx": "^2.1.2",
    "node-cron": "^3.0.2"
  },
  "files": [
    "/build/contracts/*.json",
    "/contracts/**/*.sol"
  ]
}
